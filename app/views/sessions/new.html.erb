<h1>Welcome Back</h1>
<h2>Please log in to continue</h2>

<%= form_with(url: sessions_path, local: true) do |form| %>
  <div>
    <%= form.label :email %>
    <%= form.text_field :email %>
  </div>
  <div>
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>
  <br/>
  <div>
    <%= form.submit 'Log in' %>
  </div>
<% end %>
