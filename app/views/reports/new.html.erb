   <%= date_field_tag :start_date, params[:start_date] %> </br></br>
   <%= label_tag(:search, "End Date") %>
   <%= date_field_tag :end_date, params[:end_date] %> </br></br>
   <%= submit_tag("Search") %>
<% end %>

